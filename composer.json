{
  "name": "effectiveactivism/schema-org-api",
  "description": "A Symfony bundle offering a GraphQL representation of Schema.org classes and properties",
  "homepage": "https://gitlab.com/effective-activism/schema-org-api",
  "type": "symfony-bundle",
  "license": "GPL-3.0-or-later",
  "authors": [
    {
      "name": "Peter RÃ¸nsby",
      "homepage": "https://effectiveactivism.com"
    }
  ],
  "require": {
    "php": ">=8.1",
    "effectiveactivism/sparql-client": "^10.0",
    "overblog/graphql-bundle": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0",
    "symfony/browser-kit": "^6.4",
    "symfony/framework-bundle": "^6.4",
    "symfony/phpunit-bridge": "^6.4"
  },
  "autoload": {
    "psr-4": {
      "EffectiveActivism\\SchemaOrgApi\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "EffectiveActivism\\SchemaOrgApi\\Tests\\": "tests/"
    }
  },
  "config": {
    "sort-packages": true
  },
  "replace": {
    "effectiveactivism/schema-api": "*"
  }
}
